{
  "name": "kaavapino-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@sentry/browser": "^4.4.1",
    "axios": "^0.18.0",
    "connected-react-router": "^4.5.0",
    "node-sass-chokidar": "^1.3.3",
    "oidc-client": "^1.5.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-oidc": "^3.0.2",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.83.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/index.scss --include-path ./node_modules src/index.scss -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/index.scss --include-path ./node_modules src/index.scss -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "CI=true react-scripts test --env=jsdom",
    "test-coverage": "CI=true react-scripts test --env=jsdom --coverage",
    "codecov": "codecov",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "precommit": "yarn test && yarn run lint"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "codecov": "^3.1.0",
    "deep-freeze": "0.0.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.6.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "jest-localstorage-mock": "^2.2.0",
    "npm-run-all": "^4.1.5"
  },
  "proxy": "http://localhost:8000"
}
